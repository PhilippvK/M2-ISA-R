    "version": "0.2.0",
    "configurations": [
        {
            "name": "Parse CoreDSL2 (antlr)",
            "type": "python",
            "request": "launch",
            "module": "m2isar.frontends.coredsl2.parser",
            "args": [
                "${input:inputFile}",
                "--log", "info"
            ],
            "stopOnEntry": false,
            "justMyCode": false
        },
        {
            "name": "Generate ETISS",
            "type": "python",
            "request": "launch",
            "module": "m2isar.backends.etiss.writer",
            "args": [
                //"--separate",
                //"--static-scalars",
                "--log", "info",
                "${input:inputFile}"
            ],
            "stopOnEntry": false,
            "justMyCode": false
        },
        {
            "name": "Model Viewer",
            "type": "python",
            "request": "launch",
            "module": "m2isar.backends.viewer.viewer",
            "args": [
                "${input:inputFile}"
            ],
            "stopOnEntry": false,
            "justMyCode": false
        },
        {
            "name": "Disassembler",
            "type": "python",
            "request": "launch",
            "module": "m2isar.backends.disass.disass",
            "args": [
                "${input:inputFile}",
                "RV32IMACFD",
                "../etiss_riscv_examples/install/bin/hello_world"
            ],
            "stopOnEntry": false,
            "justMyCode": false
        },
